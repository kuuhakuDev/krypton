            <!-- Tabs container -->
            <div class="flying-tabs-container">
              <!-- Tabs -->
              <div class="flying-tabs">
                <div
                  @click="switchTabs($event)"
                  class="flying-child tab-1"
                  data-tab="tab-1"
                  :class=`{
                    'is-active': activeTab === 'tab-1',
                    '': activeTab != 'tab-1'
                  }`
                >
                  <a href="#">ICO</a>
                </div>
                <div
                  @click="switchTabs($event)"
                  class="flying-child tab-2"
                  data-tab="tab-2"
                  :class=`{
                    'is-active': activeTab === 'tab-2',
                    '': activeTab != 'tab-2'
                }`
                >
                  <a href="#">TOKEN</a>
                </div>
                <div
